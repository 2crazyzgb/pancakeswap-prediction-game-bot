{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es2015",
    "sourceMap": true,
    "skipLibCheck": true,
    "stripInternal": true,
    "strictNullChecks": false,
    "importsNotUsedAsValues": "error",
    "strict": true,
    "types": ["node"],
    "lib": ["es6"],
    "outDir": "out",
  },
  "exclude": ["node_modules"]
}
